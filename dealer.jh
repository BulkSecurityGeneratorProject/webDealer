entity Contact{
name String required,
surname String required,
city String required,
address String required,
phone String required
}

entity Car{
make String required,
model String required,
price Long required,
year Long required,
gear Gear required,
version String,
capacity Double,
power Long,
color String,
mileage String,
img ImageBlob,
description TextBlob,
received Boolean required,
broken Boolean required,
}

enum Gear{
MANUAL,
AUTOMATIC
}

paginate Car with pagination
